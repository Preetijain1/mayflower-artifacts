<span class="ma__decorative-link">
  {% if decorativeLink.type == "external" %}
    <a href="{{decorativeLink.href}}" class="js-clickable-link" target="_blank">{{decorativeLink.text}}&nbsp;{% include "atoms-svg-external-link" %}</a>
  {% else %}
    <a href="{{decorativeLink.href}}" class="js-clickable-link">{{decorativeLink.text}}&nbsp;{% include "atoms-svg-arrow" %}</a>
  {% endif %}
</span>
